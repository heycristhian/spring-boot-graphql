type Diary {
    id:ID,
    date:String,
    portion:Float,
    food:Food,
    protein:Float,
    carbohydrate:Float,
    fat:Float,
    calories:Float,
    menu:String,
    user:User
}

input DiaryInput {
    id:ID,
    portion:Float,
    foodId:ID,
    menu:String,
    userId:ID
}

extend type Mutation {
    saveDiary(diaryInput:DiaryInput):Diary
    deleteDiary(id:ID!):Boolean
}

extend type Query {
    diaries:[Diary]
    diary(id:ID!):Diary
}