type User {
    id:ID,
    user:String,
    password:String,
    height:Int,
    weight:Int
}

input UserInput {
    id:ID,
    user:String,
    password:String,
    height:Int,
    weight:Int
}

extend type Mutation {
    saveUser(userInput:UserInput):User
    deleteUser(id:ID!):Boolean
}

extend type Query {
    users:[User]
    user(id:ID!):User
}